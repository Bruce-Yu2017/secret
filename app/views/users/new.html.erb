<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<form action="/users" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <label for="name">Name: <input type="text" name="user[name]" id="name"></label>
    <label for="email">Email: <input type="text" name="user[email]" id="email"></label>
    <label for="password">Password: <input type="password" name="user[password]" id="password"></label>
    <label for="passowrd_confirmation">Password_Confirmation: <input type="password" name="user[password_confirmation]" id="password_confirmation"></label>
    <input type='submit' value='Create'>
</form>
